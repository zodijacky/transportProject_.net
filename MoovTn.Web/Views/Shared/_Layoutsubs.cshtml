<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="@Url.Content("~/css/reset.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/responsive.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/bootstrap/js/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/bootstrap/js/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/main.js")" type="text/javascript"></script>
  <style>

      #tete.floatable {
          position: fixed;
          top: 10px;
           
   
    margin-top: 50px;
   
      }


  </style>
    <script>
        var positionElementInPage = $('#tete').offset().top;
        $(window).scroll(
            function() {
                if ($(window).scrollTop() >= positionElementInPage) {
                    // fixed
                    $('#tete').addClass("floatable");
                } else {
                // relative
                    $('#tete').removeClass("floatable");
            }
        }
);



    </script>
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @RenderSection("Styles", false)
    @RenderSection("Script", false)
    <script src='https://www.google.com/recaptcha/api.js'></script>
</head>
<body>
    <section class="hero"> 
      
        <section class="listings">
            <div class="wrapper" id="ajaxid">
                @RenderBody()

            </div>
        </section>	<!--  end listing section  -->
    </section><!--  end hero section  -->

  



    <footer>
   

        <div class="copyrights wrapper">
            Copyright © 2015 Amine Hatem. All Rights Reserved.
        </div>
    </footer><!--  end footer  -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
